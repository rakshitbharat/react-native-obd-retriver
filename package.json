{
  "name": "react-native-obd-retriver",
  "version": "1.0.2",
  "description": "A React Native library for raw OBD-II data communication via Bluetooth LE",
  "main": "lib/commonjs/index.js",
  "module": "lib/module/index.js",
  "types": "lib/typescript/index.d.ts",
  "source": "src/index.ts",
  "react-native": "lib/module/index.js",
  "exports": {
    ".": {
      "types": "./lib/typescript/index.d.ts",
      "import": "./lib/module/index.js",
      "require": "./lib/commonjs/index.js",
      "default": "./lib/commonjs/index.js"
    }
  },
  "files": [
    "lib",
    "!lib/**/*.map",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "!**/*.test.*",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "bob build && tsc --build tsconfig.build.json",
    "clean": "rimraf lib coverage",
    "prepare": "husky",
    "prepublishOnly": "npm run clean && npm run build",
    "test": "jest --config jest.config.cjs --coverage --passWithNoTests",
    "test:watch": "jest --watch",
    "lint": "eslint \"./src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint \"./src/**/*.{ts,tsx}\" --fix",
    "lint:check": "eslint \"./src/**/*.{ts,tsx}\"",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx}\"",
    "format:src": "prettier --write \"src/**/*.{ts,tsx}\"",
    "typecheck": "tsc --noEmit",
    "typecheck:watch": "tsc --noEmit --watch",
    "validate": "npm-run-all --parallel typecheck lint:check format:check test",
    "fix-all": "npm-run-all --sequential format lint:fix",
    "release": "release-it",
    "lint:fix:all": "eslint \"./src/**/*.{ts,tsx}\" --fix && prettier --write \"src/**/*.{ts,tsx}\""
  },
  "keywords": [
    "react-native",
    "ios",
    "android",
    "bluetooth",
    "ble",
    "obd",
    "obd2",
    "elm327",
    "automotive"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rakshitbharat/react-native-obd-retriver.git"
  },
  "author": "rakshitbharat",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rakshitbharat/react-native-obd-retriver/issues"
  },
  "homepage": "https://github.com/rakshitbharat/react-native-obd-retriver#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-native": ">=0.72.0",
    "react-native-bluetooth-obd-manager": ">=1.0.2",
    "react-native-permissions": ">=3.8.0"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-native-beautiful-logs": "^1.0.5",
    "react-native-bluetooth-obd-manager": "^1.0.2",
    "react-native-permissions": ">=3.8.0",
    "text-encoding": "^0.7.0",
    "typescript-eslint": "^8.29.1"
  },
  "devDependencies": {
    "@babel/core": "^7.24.0",
    "@babel/runtime": "^7.24.0",
    "@release-it/conventional-changelog": "^10.0.0",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^12.4.3",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "@types/react": "^18.2.64",
    "@types/react-native": "^0.73.0",
    "@types/text-encoding": "^0.0.40",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint": "^9.24.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^4.3.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-n": "^17.17.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-react": "^7.34.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-native": "^4.1.0",
    "eslint-plugin-security": "^3.0.1",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^15.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.5",
    "react-native-builder-bob": "^0.23.2",
    "release-it": "^18.1.2",
    "rimraf": "^5.0.5",
    "typescript": "~5.5.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings=0 --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "jest": {
    "preset": "react-native",
    "setupFilesAfterEnv": [
      "@testing-library/jest-native/extend-expect"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      [
        "commonjs",
        {
          "copyFlow": false,
          "includeExtensions": true
        }
      ],
      [
        "module",
        {
          "copyFlow": false,
          "includeExtensions": true
        }
      ],
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e"
}
